{
  "compilerOptions": {
    "target": "es2023",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "@/hooks": [
        "./src/hooks"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/components": [
        "./src/app/components"
      ],
      "@/components/*": [
        "./src/app/components/*"
      ],
    }
  },
  "include": [
    "src"
  ]
}
